{{!
    @templateblock_timetable/main

    The purpose of this template is to render the timetable block.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Example context (json):
    {
    }
}}

<div class="d-flex justify-content-end mb-3" id="yui_{{uniqid}}">
  </div>
    <div class="events card-text content calendarwrapper"  data-context-id="{{defaulteventcontext}}" data-courseid="{{courseid}}" data-categoryid="{{categoryid}}">

    {{> block_timetable/event}}

    </div>
{{#pagination}}
<div>

    {{#prev}}
        <a href="#" data-url="{{{prevurl}}}" data-mode="{{ulayout}}" class="timeblock{{instanceid}}" >
            <span aria-hidden="true">«</span>
           {{prevtext}}
        </a>
    {{/prev}}
    
    {{{sep}}}
    {{#next}}
       <a href="#"  data-url="{{{nexturl}}}"  data-mode="{{ulayout}}" class="timeblock{{instanceid}}">
            
           {{nexttext}} <span aria-hidden="true">»</span>
        </a>
    {{/next}}
</div>
{{/pagination}}


